<div class="card-body table-responsive">
  <% breadcrumb :carros %>
  <h4 class="card-title">Carros</h4>
  <table class="table table-hover">
    <thead>
      <tr>
          <th><strong>Placa</strong></th>
          <th><strong>Nombre propietario</strong></th>
          <th><strong>Cedula</strong></th>
          <th><strong>Fecha vencimiento soat</strong></th>
          <th><strong>Fecha vencimiento tecnomecanica</strong></th>
          <th><strong>Acciones</strong></th>
      </tr>
    </thead>

    <tbody>
      <%= render partial: "carros", collection: @carros, as: :carro %>
    </tbody>
  </table>
</div>
